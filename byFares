local disableAnimButton = Instance.new("TextButton", utilityPage)
disableAnimButton.Name = "DisableAnimButton"
disableAnimButton.Size = UDim2.new(0, 200, 0, 50)
disableAnimButton.Position = UDim2.new(0.5, -100, 0, 200) 
disableAnimButton.BackgroundColor3 = Color3.fromRGB(255, 100, 50)
disableAnimButton.Text = "Disable Animation [OFF]"
disableAnimButton.Font = Enum.Font.SourceSansBold
disableAnimButton.TextSize = 20
disableAnimButton.TextColor3 = Color3.fromRGB(255, 255, 255)

local animDisabled = false
local HumanAnimCons = {}
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function disableAnimate(Char)
	local Animate = Char:FindFirstChild("Animate")
	if Animate then
		Animate.Disabled = true
	end
end

local function enableAnimate(Char)
	local Animate = Char:FindFirstChild("Animate")
	if Animate then
		Animate.Disabled = false
	end
end

disableAnimButton.MouseButton1Click:Connect(function()
	animDisabled = not animDisabled
	if animDisabled then
		disableAnimButton.BackgroundColor3 = Color3.fromRGB(50, 255, 50)
		disableAnimButton.Text = "Disable Animation [ON]"
		if player.Character then
			disableAnimate(player.Character)
		end
		HumanAnimCons.charAdded = player.CharacterAdded:Connect(function(newChar)
			disableAnimate(newChar)
		end)
	else
		disableAnimButton.BackgroundColor3 = Color3.fromRGB(255, 100, 50)
		disableAnimButton.Text = "Disable Animation [OFF]"
		if player.Character then
			enableAnimate(player.Character)
		end
		if HumanAnimCons.charAdded then
			HumanAnimCons.charAdded:Disconnect()
			HumanAnimCons.charAdded = nil
		end
	end
end)

