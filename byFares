local antiFlingButton = Instance.new("TextButton", playerPage); antiFlingButton.Name = "AntiFlingButton"; antiFlingButton.Size = UDim2.new(0, 200, 0, 40); antiFlingButton.Position = UDim2.new(0.5, -100, 0, 80); antiFlingButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50); antiFlingButton.Text = "Anti-Fling 75-80% [OFF]"; antiFlingButton.Font = Enum.Font.SourceSansBold; antiFlingButton.TextColor3 = Color3.fromRGB(255, 255, 255); antiFlingButton.TextSize = 18
function toggleAntiFling() antiFlingEnabled = not antiFlingEnabled; if antiFlingEnabled then antiFlingButton.BackgroundColor3 = Color3.fromRGB(50, 255, 50); antiFlingButton.Text = "Anti-Fling 75-80% [ON]"; stopAntiFlingFunc = loadstring(game:HttpGet("https://raw.githubusercontent.com/FaresXForest/anti-fling/refs/heads/main/antflingv2"))() else antiFlingButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50); antiFlingButton.Text = "Anti-Fling 75-80% [OFF]"; if stopAntiFlingFunc then pcall(stopAntiFlingFunc) end; local char = localPlayer.Character or localPlayer.CharacterAdded:Wait(); char:FindFirstChild("Humanoid").Health = 0 end end
antiFlingButton.MouseButton1Click:Connect(toggleAntiFling)
