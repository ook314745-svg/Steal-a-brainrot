if localPlayer.Character then createGlowingBeam(localPlayer.Character) end
localPlayer.CharacterAdded:Connect(createGlowingBeam)
end)
local autoRefreshButton = Instance.new("TextButton", mainPage); autoRefreshButton.Name = "ToggleAutoRefresh"; autoRefreshButton.Size = UDim2.new(0, 180, 0, 40); autoRefreshButton.Position = UDim2.new(0.5, -90, 0, 20); autoRefreshButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50); autoRefreshButton.Text = "Timer ESP [OFF]"; autoRefreshButton.Font = Enum.Font.SourceSansBold; autoRefreshButton.TextColor3 = Color3.fromRGB(255, 255, 255); autoRefreshButton.TextSize = 18
function cleanupClones() for _, plot in ipairs(workspace.Plots:GetChildren()) do if plot:FindFirstChild("FaresCloneGUI") then plot.FaresCloneGUI:Destroy() end end end
function createClones() for _, plot in ipairs(workspace.Plots:GetChildren()) do local mainObject = plot:FindFirstChild("Purchases") and plot.Purchases:FindFirstChild("PlotBlock") and plot.Purchases.PlotBlock:FindFirstChild("Main") if mainObject then local realTimerGui = mainObject:FindFirstChild("BillboardGui") if realTimerGui then local cloneGui = realTimerGui:Clone(); cloneGui.Name = "FaresCloneGUI"; cloneGui.Parent = plot; cloneGui.Adornee = mainObject; cloneGui.StudsOffset = Vector3.new(0, 15, 0); cloneGui.AlwaysOnTop = true; cloneGui.MaxDistance = math.huge; cloneGui.Size = UDim2.new(0, 400, 0, 100); if cloneGui:FindFirstChild("Main") then cloneGui.Main.TextSize = 100; cloneGui.Main.TextStrokeTransparency = 0 end; if cloneGui:FindFirstChild("Locked") then cloneGui.Locked.TextSize = 80; cloneGui.Locked.TextStrokeTransparency = 0 end end end end end
function startAutoRefresher() if autoRefreshCoroutine then coroutine.close(autoRefreshCoroutine) end; autoRefreshCoroutine = coroutine.create(function() while features.AutoRefreshActive do cleanupClones(); wait(0.1); createClones(); wait(1) end; cleanupClones() end); coroutine.resume(autoRefreshCoroutine) end
function toggleAutoRefresh() features.AutoRefreshActive = not features.AutoRefreshActive; autoRefreshButton.BackgroundColor3 = features.AutoRefreshActive and Color3.fromRGB(50, 255, 50) or Color3.fromRGB(255, 50, 50); autoRefreshButton.Text = features.AutoRefreshActive and "Timer ESP [ON]" or "Timer ESP [OFF]"; if features.AutoRefreshActive then startAutoRefresher() else if autoRefreshCoroutine then coroutine.close(autoRefreshCoroutine); autoRefreshCoroutine = nil end; cleanupClones() end end
autoRefreshButton.MouseButton1Click:Connect(toggleAutoRefresh)
