task.spawn(function()
	while true do
		if running then
			local char = player.Character
			if char and char:FindFirstChild("HumanoidRootPart") then

				for _,v in pairs(workspace:GetDescendants()) do
					if v:IsA("ProximityPrompt") then
						pcall(function()
							v.HoldDuration = 0
							if v.Parent and v.Parent:IsA("BasePart") then
								if (v.Parent.Position - char.HumanoidRootPart.Position).Magnitude <= v.MaxActivationDistance then
									fireproximityprompt(v)
								end
							end
						end)
					end
				end

				if workspace:FindFirstChild("Plots") then
					for _,plot in pairs(workspace.Plots:GetChildren()) do
						if plot:FindFirstChild("AnimalPodiums") then
							for _,podium in pairs(plot.AnimalPodiums:GetChildren()) do
								local prompt = podium:FindFirstChild("Base")
									and podium.Base:FindFirstChild("Spawn")
									and podium.Base.Spawn:FindFirstChild("PromptAttachment")
									and podium.Base.Spawn.PromptAttachment:FindFirstChild("ProximityPrompt")

								if prompt then
									pcall(function()
										prompt.HoldDuration = 0
										if (prompt.Parent.Parent.Position - char.HumanoidRootPart.Position).Magnitude <= prompt.MaxActivationDistance then
											fireproximityprompt(prompt)
										end
									end)
								end
							end
						end
					end
				end

			end
		end
		task.wait(0.05)
	end
end)

btn.MouseButton1Click:Connect(function()
	running = not running

	if running then
		btn.Text = "Auto Steal : ON"
		btn.BackgroundColor3 = Color3.fromRGB(120,70,180)
	else
		btn.Text = "Auto Steal : OFF"
		btn.BackgroundColor3 = Color3.fromRGB(90,40,130)
	end
end)

local autoJumpButton = Instance.new("TextButton", pvpPage)
autoJumpButton.Name = "AutoJumpButton"
autoJumpButton.Size = UDim2.new(0, 200, 0, 40)
autoJumpButton.Position = UDim2.new(0.5, -100, 0, 20)
autoJumpButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
autoJumpButton.Text = "AutoJump [OFF]"
autoJumpButton.Font = Enum.Font.SourceSansBold
autoJumpButton.TextColor3 = Color3.fromRGB(255, 255, 255)
autoJumpButton.TextSize = 18

local autoJumpEnabled = false
local HumanModCons = {}

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

local function setupAutoJump(Char)
	local Human = Char:FindFirstChildWhichIsA("Humanoid")
	if not Human then return end

	local function autoJumpFunc()
		if not (Char and Human and Human.RootPart) then return end
		local pos = Human.RootPart.Position
		local look = Human.RootPart.CFrame.LookVector

		local check1 = workspace:FindPartOnRay(Ray.new(pos - Vector3.new(0,1.5,0), look * 3), Human.Parent)
		local check2 = workspace:FindPartOnRay(Ray.new(pos + Vector3.new(0,1.5,0), look * 3), Human.Parent)

		if check1 or check2 then
			Human.Jump = true
		end
	end

	HumanModCons.ajLoop = RunService.RenderStepped:Connect(autoJumpFunc)
end

local function removeAutoJump()
	if HumanModCons.ajLoop then
		HumanModCons.ajLoop:Disconnect()
		HumanModCons.ajLoop = nil
	end
end

autoJumpButton.MouseButton1Click:Connect(function()
	autoJumpEnabled = not autoJumpEnabled
	if autoJumpEnabled then
		autoJumpButton.BackgroundColor3 = Color3.fromRGB(50, 255, 50)
		autoJumpButton.Text = "AutoJump [ON]"
		if player.Character then
			setupAutoJump(player.Character)
		end
		HumanModCons.ajCA = player.CharacterAdded:Connect(function(newChar)
			if HumanModCons.ajLoop then HumanModCons.ajLoop:Disconnect() end
			setupAutoJump(newChar)
		end)
	else
		autoJumpButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
		autoJumpButton.Text = "AutoJump [OFF]"
		removeAutoJump()
		if HumanModCons.ajCA then
			HumanModCons.ajCA:Disconnect()
			HumanModCons.ajCA = nil
		end
	end
end)




local hitboxButton = Instance.new("TextButton", pvpPage)
hitboxButton.Name = "HitboxButton"
hitboxButton.Size = UDim2.new(0, 180, 0, 40)
hitboxButton.Position = UDim2.new(0.5, -90, 0, 69)
hitboxButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
hitboxButton.Text = "Hitbox [OFF]"
hitboxButton.Font = Enum.Font.SourceSansBold
hitboxButton.TextColor3 = Color3.fromRGB(255, 255, 255)
hitboxButton.TextSize = 18

local hitboxEnabled = false
local originalSizes = {} 

hitboxButton.MouseButton1Click:Connect(function()
    hitboxEnabled = not hitboxEnabled
    if hitboxEnabled then
        hitboxButton.Text = "Hitbox [ON]"
        hitboxButton.BackgroundColor3 = Color3.fromRGB(50, 255, 50)
        
        
        local Players = game:GetService("Players")
        local speaker = Players.LocalPlayer
        local args = {"all", "16", "0.4"}
        
        local function getPlayersByName(name)
            local list = {}
            if name:lower() == "all" then
                for _, plr in pairs(Players:GetPlayers()) do
                    if plr ~= speaker then
                        table.insert(list, plr)
                    end
                end
            else
                local plr = Players:FindFirstChild(name)
                if plr then
                    table.insert(list, plr)
                end
            end
            return list
        end

        local playersToHit = getPlayersByName(args[1])
        local sizeArg = tonumber(args[2]) or 16
        local transparency = tonumber(args[3]) or 0.4

        for _, plr in pairs(playersToHit) do
            if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                local Root = plr.Character.HumanoidRootPart
                if Root:IsA("BasePart") then
                    originalSizes[plr] = {Size = Root.Size, Transparency = Root.Transparency}
                    Root.Size = Vector3.new(sizeArg, sizeArg, sizeArg)
                    Root.Transparency = transparency
                    Root.CanCollide = false
                end
            end
        end
    else
        
        hitboxButton.Text = "Hitbox [OFF]"
        hitboxButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)

        for plr, data in pairs(originalSizes) do
            if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                local Root = plr.Character.HumanoidRootPart
                Root.Size = data.Size
                Root.Transparency = data.Transparency
                Root.CanCollide = true
            end
        end
        originalSizes = {}
    end
end) 


task.spawn(function()
local PlotsFolder = workspace:WaitForChild("Plots")
local function findMyPlot() for _, plot in ipairs(PlotsFolder:GetChildren()) do local podiums = plot:FindFirstChild("AnimalPodiums") if podiums then for _, pod in ipairs(podiums:GetChildren()) do local claim = pod:FindFirstChild("Claim") if claim then local hitbox = claim:FindFirstChild("Hitbox") if hitbox then local touch = hitbox:FindFirstChild("TouchInterest") if touch then return plot end end end end end end; return nil end
local MyPlot = findMyPlot()
if not MyPlot then return end
local function findYourBaseLabel(plot) for _, obj in ipairs(plot:GetDescendants()) do if obj:IsA("TextLabel") and obj.Text then if string.lower(obj.Text):find("your base") or string.lower(obj.Text):find("yourbase") then return obj end end end; return nil end
local lbl = findYourBaseLabel(MyPlot)
if not lbl then return end
local function getWorldPartFromLabel(label) local gui = label:FindFirstAncestorWhichIsA("SurfaceGui") or label:FindFirstAncestorWhichIsA("BillboardGui"); if gui and gui.Adornee then return gui.Adornee end; local p = label.Parent; while p do if p:IsA("BasePart") then return p end; p = p.Parent end; return nil end
local basePart = getWorldPartFromLabel(lbl)
if not basePart then return end
