local infiniteJumpButton = Instance.new("TextButton", playerPage); infiniteJumpButton.Name = "InfiniteJumpButton"; infiniteJumpButton.Size = UDim2.new(0, 200, 0, 40); infiniteJumpButton.Position = UDim2.new(0.5, -100, 0, 170); infiniteJumpButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50); infiniteJumpButton.Text = "Infinite Jump [OFF]"; infiniteJumpButton.Font = Enum.Font.SourceSansBold; infiniteJumpButton.TextColor3 = Color3.fromRGB(255, 255, 255); infiniteJumpButton.TextSize = 18; local isJumpActive = false; local jumpConnection = nil; local function activateJump() if jumpConnection then return end; jumpConnection = game:GetService("UserInputService").JumpRequest:Connect(function() if isJumpActive and localPlayer.Character then local rootPart = localPlayer.Character:FindFirstChild("HumanoidRootPart") if rootPart then rootPart.Velocity = Vector3.new(rootPart.Velocity.X, 50, rootPart.Velocity.Z) end end end) end; local function deactivateJump() if jumpConnection then jumpConnection:Disconnect(); jumpConnection = nil end end; infiniteJumpButton.MouseButton1Click:Connect(function() isJumpActive = not isJumpActive; if isJumpActive then infiniteJumpButton.BackgroundColor3 = Color3.fromRGB(50, 255, 50); infiniteJumpButton.Text = "Infinite Jump [ON]"; activateJump() else infiniteJumpButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50); infiniteJumpButton.Text = "Infinite Jump [OFF]"; deactivateJump() end end)







local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer


local circle = Instance.new("Part")
circle.Name = "GroundCircle"
circle.Shape = Enum.PartType.Cylinder
circle.Size = Vector3.new(0.2, 33, 33) 
circle.Color = Color3.fromRGB(0, 255, 0) 
circle.Material = Enum.Material.Neon
circle.Transparency = 0.3
circle.Anchored = true
circle.CanCollide = false
circle.Parent = workspace


circle.Orientation = Vector3.new(0, 0, 90)

RunService.RenderStepped:Connect(function()
    if player.Character 
    and player.Character:FindFirstChild("HumanoidRootPart") then
        
        local hrp = player.Character.HumanoidRootPart
        
     



       circle.Position = Vector3.new(
            hrp.Position.X,
            hrp.Position.Y - 3, 
            hrp.Position.Z
        )
    end
end)

