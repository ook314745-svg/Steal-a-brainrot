local aegisEnabled = false
local touchConnections = {}

local aegisButton = Instance.new("TextButton", playerPage)
aegisButton.Name = "AegisButton"
aegisButton.Size = UDim2.new(0, 240, 0, 50)
aegisButton.Position = UDim2.new(0.5, -120, 0, 20)
aegisButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
aegisButton.Text = "antiRagdoll (shouldhold bat) Mode [OFF]"
aegisButton.Font = Enum.Font.SourceSansBold
aegisButton.TextSize = 18
aegisButton.TextColor3 = Color3.fromRGB(255, 255, 255)

local THREAT_TOOL_NAMES = {
["Bat"] = true,
["Medusa's Head"] = true,
}

local function onPartTouched(hit)

local tool = hit:FindFirstAncestorOfClass("Tool")  
if tool and THREAT_TOOL_NAMES[tool.Name] then  
    local char = localPlayer.Character  
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end  
      
    local hrp = char.HumanoidRootPart  
      
      
    if hrp.Anchored then return end  

      
    hrp.Anchored = true  
      
      
    task.wait()   
    hrp.Anchored = false  
end

end

local function setupAegis(character)

for _, connection in ipairs(touchConnections) do  
    connection:Disconnect()  
end  
touchConnections = {}  

  
for _, part in ipairs(character:GetChildren()) do  
    if part:IsA("BasePart") then  
        table.insert(touchConnections, part.Touched:Connect(onPartTouched))  
    end  
end

end

local function removeAegis()
for _, connection in ipairs(touchConnections) do
connection:Disconnect()
end
touchConnections = {}
end

local function toggleAegis()
aegisEnabled = not aegisEnabled
if aegisEnabled then
aegisButton.BackgroundColor3 = Color3.fromRGB(255, 215, 0) -- Aegis Gold
aegisButton.Text = "antiRagdoll Mode [ON]"
if localPlayer.Character then
setupAegis(localPlayer.Character)
end
else
aegisButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
aegisButton.Text = "Anti-Ragdoll ( u should hold bat 24h)[OFF]"
removeAegis()
end
end

aegisButton.MouseButton1Click:Connect(toggleAegis)

localPlayer.CharacterAdded:Connect(function(character)
if aegisEnabled then
setupAegis(character)
end
end)

