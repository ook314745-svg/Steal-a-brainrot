	if not autoBatEnabled then return end
	local char = player.Character
	if not char then return end
	local tool = char:FindFirstChildOfClass("Tool")
	if tool and tool.Name == ToolName then
		pcall(function()
			tool:Activate()
		end)
	end
end)



local player = game.Players.LocalPlayer

local running = false
local parentFrame = pvpPage 

local btn = Instance.new("TextButton", parentFrame)
btn.Size = UDim2.fromScale(0.1, 0.8)
btn.Text = "Auto Steal : OFF"
btn.TextColor3 = Color3.fromRGB(255,255,255)
btn.TextScaled = true
btn.Font = Enum.Font.GothamBold
btn.BackgroundColor3 = Color3.fromRGB(90,40,130)
btn.BorderSizePixel = 0

